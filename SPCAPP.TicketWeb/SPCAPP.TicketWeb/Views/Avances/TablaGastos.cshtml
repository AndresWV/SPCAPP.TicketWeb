@model IEnumerable<SPCAPP.TicketWeb.Models.RendTk>
@if (Model.Count() > 0)
{
    <table class="ml-3 table table-bordered table-hover w-100">
        <thead class="table-striped">
            <tr class="table-striped text-white small">
                <td class="text-uppercase">@Html.DisplayNameFor(m => m.Fecha)</td>
                <td class="text-uppercase">@Html.DisplayNameFor(m => m.GastoId)</td>
                <td class="text-uppercase">@Html.DisplayNameFor(m => m.Cantidad)</td>
                <td class="text-uppercase">@Html.DisplayNameFor(m => m.Total)</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="w-100">
                    <td class="text-uppercase">@item.Fecha</td>
                    <td class="text-uppercase">@item.GastoId</td>
                    <td class="text-uppercase">@item.Cantidad</td>
                    <td class="text-uppercase">@item.Total</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No existen registros</p>
}