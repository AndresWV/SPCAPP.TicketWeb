@model SPCAPP.TicketWeb.Models.TicketSpc
<!--TRABAJO REALIZADO-->
<div class="row">
    <!--checks-->
    <div class="row">
        <div class="col-6 form-check">
            <label class="form-check-label textForm3 ml-3 mr-5" for="trjProgramado">TRABAJO REALIZADO</label>
            <input class="form-check-input ml-3" type="checkbox" id="trjR" asp-for="Realizado" />
        </div>
        <div class="col-3 form-check">
            <label class="form-check-label textForm3 ml-3 mr-3" for="trjProgramado">CON OT</label>
            <input class="form-check-input ml-1" type="checkbox" id="ot" />
        </div>
        <div class="col-3 form-check">
            <label class="form-check-label textForm3 ml-3 mr-3" for="trjProgramado">CON TW</label>
            <input class="form-check-input ml-1" type="checkbox" id="tw" asp-for="Tw" />
        </div>
    </div>
    <div class="row mt-3">
        <div class="col-7">
            <div class="col-5 float-left ">
                <div class="row">
                    <div class="form-outline">
                        <input type="text" id="nOrden" class="form-control" readonly />
                        <label class="form-label" for="nOrden">N° ORDEN</label>
                    </div>
                </div>
                <div class="row mt-3">
                    <input type="date" id="dateTrabRlz" name="trip-start" asp-for="FechaCierre" />
                </div>
            </div>
            <div class="col-5 float-right">
                <div class="row">
                    <input type="time" id="hraIni" name="appt" required asp-for="HrsInicio">
                </div>
                <div class="row mt-3">
                    <input type="time" id="hraTerm" name="appt2" required asp-for="HrsTermino">
                </div>
            </div>
        </div>
        <div class="col-5">
            <div class="col-5 float-lg-left">
                <div class="row">
                    <div class="row">
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" name="visitaCheckk" id="visitaCheck" asp-for="Visita">
                            <label class="form-check-label" for="visitaCheck">
                                VISITA
                            </label>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" name="remotoCheckk" id="remotoCheck" asp-for="Remoto">
                            <label class="form-check-label" for="remotoCheck">
                                REMOTO
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-5 float-lg-right">
                <div class="row float-lg-right">
                    <div class="row">
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" name="tallerCheckk" id="tallerCheck" asp-for="Taller">
                            <label class="form-check-label" for="tallerCheck">
                                TALLER
                            </label>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" name="telCheckk" id="telCheck" asp-for="Telefono">
                            <label class="form-check-label" for="telCheck">
                                TELEFONICO
                            </label>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="row mt-3">
        <div class="form-group">
            <textarea class="form-control" id="trabaRejoDesc" rows="7" asp-for="Trabajo"></textarea>
            <label for="trabaRejoDesc">DESCRIPCION DEL TRABAJO REALIZADO</label>
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-5">
            <button type="button" onclick="cargarGastos()" class="btn coloAction ml-3">GASTOS</button>
        </div>
        <div class="col-7">
            <select class="form-select float-lg-right" id="asignadoo2" aria-label="Default select example" asp-for="TerminadoPor">
                ASIGNADO
            </select>
        </div>
    </div>
    <!--Tabla de gastos-->
    <div class="row mt-3" id="gastosVista" style="display:none"></div>
</div>